    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "watch": "vite",
    "build": "vite build && cp src/style.css dist/style.css",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.2.1",
    "esbuild": "^0.25.0",
    "vite": "^6.2.4"
  },
  "peerDependencies": {
    "vue": "^3.5.13"
  }
}
